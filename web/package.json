  "type": "module",
  "alias": {
    "src": "./src",
    "utils": "./src/utils",
    "assets": "./src/assets",
    "components": "./src/components",
    "connectors": "./src/connectors",
    "context": "./src/context",
    "layout": "./src/layout",
